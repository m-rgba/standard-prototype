    "@storybook/addons": "^5.3.19",
    "@storybook/svelte": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2"
  },
  "dependencies": {
