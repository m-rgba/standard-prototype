{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "concurrently --kill-others \"rollup -c -w\" \"start-storybook -s ./public -p 8081\"",
    "start": "sirv public -s -p 8080",
